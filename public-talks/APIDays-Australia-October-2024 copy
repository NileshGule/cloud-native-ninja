# Commands related to k8sUG Melbourne 

# Add k8sGPT backend

```bash

k8sgpt auth add \
--backend azureopenai \
--baseurl https://k8sgpt-australia-azure-openai.openai.azure.com/ \
--engine k8sgptauazureopenai \
--model gpt-35-turbo

k8sgpt auth add \
--backend localai \
--baseurl http://localhost:11434/v1 \
--model llama3.1

k8sgpt auth add \
--backend localai \
--baseurl http://localhost:11434/v1 \
--model phi3

k8sgpt auth add \
--backend localai \
--baseurl http://localhost:11434/v1 \
--model mistral

k8sgpt auth add \
--backend localai \
--baseurl http://localhost:1234/v1 \
--model lmstudio-community/Meta-Llama-3.1-8B-Instruct-GGUF/Meta-Llama-3.1-8B-Instruct-Q4_K_M.gguf

docker run --detach \
--publish 3000:8080 \
--add-host=host.docker.internal:host-gateway \
--volume open-webui:/app/backend/data \
--name open-webui \
--restart always \
ghcr.io/open-webui/open-webui:main

```

k8sgpt auth default --provider azureopenai

k8sgpt auth -h

k8sgpt auth list

k8sgpt auth remove --backends azureopenai

k8sgpt analyse --explain

k8sgpt analyse --explain --with-doc

k8sgpt analyse --explain --language French

k8sgpt analyse --explain --language Chinese

k8sgpt analyse --explain --language Japanese

k8sgpt analyse --explain --language Korean

k8sgpt analyse --explain --language Spanish

k8sgpt integration list

k8sgpt integration activate trivy

k8sgpt integration deactivate trivy

k8sgpt filters list

k8sgpt analyse --explain --filter VulnerabilityReport

k8sgpt analyse --explain --filter ConfigAuditReport

VulnerabilityReport (integration)
ConfigAuditReport (integration)

